{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard"
  ],
  "automerge": true,
  "automergeType": "branch",
  "schedule": [ "* * * * 0,6" ],
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": [ "* * * * 0,6" ]
  },
  "ignoreDeps": ["scipy"] // dl4to is not up to date, so we cannot update some deps
}
